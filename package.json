{
  "name": "tppc-faqbot",
  "version": "1.0.0",
  "description": "",
  "main": "bot.js",
  "type": "module",
  "scripts": {
    "start": "node bot.js",
    "start:dev": "DOTENV_CONFIG_PATH=.env.development.local node bot.js",
    "start:dev:docker": "./scripts/start-dev.sh",
    "pause:dev": "pkill -f 'node bot.js' && docker pause tppc-mysql",
    "resume:dev": "docker unpause tppc-mysql && DOTENV_CONFIG_PATH=.env.development.local node bot.js",
    "stop:dev:docker": "./scripts/stop-dev.sh",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "fuse.js": "^7.1.0",
    "mysql2": "^3.16.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.16",
    "vitest": "^4.0.16"
  }
}
